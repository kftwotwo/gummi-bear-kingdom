<%= form_for [@user] do |f| %>
  <%= f.label :first_name %>
  <%= f.text_field :first_name %>
  <%= f.label :last_name %>
  <%= f.text_field :last_name %>
  <%= f.label :email %>
  <%= f.email_field :email %>
  <%= f.label :pic %>
  <%= f.text_field :pic %>
  <%= f.label :password %>
  <%= f.password_field :password %>
  <p><%= f.submit %></p>
<% end %>
